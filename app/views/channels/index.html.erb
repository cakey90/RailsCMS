<h1>Listing channels</h1>

<table>
  <thead>
    <tr>
      <th>Channelname</th>
      <th>Fid</th>
      <th>Sort</th>
      <th>Url</th>
      <th>Imagesrc</th>
      <th>Filesrc</th>
      <th>Videosrc</th>
      <th>Hits</th>
      <th>Content</th>
      <th>Tags</th>
      <th>Description</th>
      <th>Addtime</th>
      <th>Group</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @channels.each do |channel| %>
      <tr>
        <td><%= channel.channelName %></td>
        <td><%= channel.fid %></td>
        <td><%= channel.sort %></td>
        <td><%= channel.url %></td>
        <td><%= channel.imageSrc %></td>
        <td><%= channel.fileSrc %></td>
        <td><%= channel.videoSrc %></td>
        <td><%= channel.hits %></td>
        <td><%= channel.content %></td>
        <td><%= channel.tags %></td>
        <td><%= channel.description %></td>
        <td><%= channel.addtime %></td>
        <td><%= channel.group %></td>
        <td><%= channel.status %></td>
        <td><%= link_to 'Show', channel %></td>
        <td><%= link_to 'Edit', edit_channel_path(channel) %></td>
        <td><%= link_to 'Destroy', channel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Channel', new_channel_path %>
